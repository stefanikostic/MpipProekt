<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent">

    <!-- TODO: Update blank fragment layout -->
    <androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".RegisterActivity"
        android:background="@drawable/pozz">

        <EditText
                android:id="@+id/reg_ime"
                android:layout_width="281dp"
                android:layout_height="55dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="100dp"
                android:layout_marginEnd="8dp"
                android:paddingLeft="20dp"
                android:ems="10"
                android:hint="Name"
                android:background="@drawable/darkbackwithborder"
                android:inputType="textPersonName"
                android:textColor="#ffffff"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:fontFamily="sans-serif-medium"
                app:layout_constraintTop_toTopOf="parent"/>

        <EditText
                android:id="@+id/reg_surname"
                android:layout_width="281dp"
                android:layout_height="55dp"
                android:layout_marginStart="8dp"
                android:layout_marginEnd="8dp"
                android:ems="10"
                android:hint="Surname"
                android:paddingLeft="20dp"
                android:inputType="textPersonName"
                android:textColor="#ffffff"
                android:background="@drawable/darkbackwithborder"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintHorizontal_bias="0.52"
                android:layout_marginTop="16dp"
                android:fontFamily="sans-serif-medium"
                app:layout_constraintTop_toBottomOf="@+id/reg_ime"/>

        <EditText
                android:id="@+id/reg_tel"
                android:layout_width="281dp"
                android:layout_height="55dp"
                android:ems="10"
                android:background="@drawable/darkbackwithborder"
                android:textColor="#ffffff"
                android:textColorHint="#ffffff"
                android:hint="Telephone"
                android:paddingLeft="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/reg_surname"
                android:fontFamily="sans-serif-medium"
                android:layout_marginTop="16dp"/>


        <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/fab"
                android:background="@color/red"
                android:layout_width="74dp"
                android:layout_height="62dp"
                app:srcCompat="@drawable/ic_keyboard_arrow_right_black_24dp" tools:ignore="VectorDrawableCompat"
                android:layout_marginStart="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.927"
                app:backgroundTint="@color/colorPrimaryDark"
                android:layout_marginTop="76dp" app:layout_constraintTop_toBottomOf="@+id/reg_tel"/>

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>