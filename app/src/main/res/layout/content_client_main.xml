<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/activity_client_main"
        tools:context=".ClientMainActivity">

    <TextView
        android:text="Available bicycles"
        android:layout_width="354dp"
        android:layout_height="62dp" android:id="@+id/textView3" app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/gridview_bikes" android:layout_marginStart="16dp"
        android:layout_marginTop="16dp" android:textSize="30sp" android:fontFamily="sans-serif-condensed"
        app:layout_constraintEnd_toEndOf="parent"
        android:paddingStart="20dp"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        app:layout_constraintHorizontal_bias="1.0" app:layout_constraintVertical_bias="0.0"
        android:layout_marginEnd="8dp" android:lineSpacingExtra="8sp" android:typeface="sans"
        android:autoText="false" android:textAppearance="@style/TextAppearance.AppCompat.Body1"/>
    <View
        android:id="@+id/divider"
        android:layout_width="412dp"
        android:layout_height="2dp"
        android:background="#C48740"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="1.0"
        android:layout_marginStart="0dp" app:layout_constraintTop_toBottomOf="@+id/textView3"/>
    <GridView
        android:id="@+id/gridview_bikes1"
        android:layout_width="399dp"
        android:layout_height="617dp"
        android:visibility="visible" app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="1dp"
        app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="1dp"
        app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="1dp"
        app:layout_constraintHorizontal_bias="0.347"/>

</androidx.constraintlayout.widget.ConstraintLayout>